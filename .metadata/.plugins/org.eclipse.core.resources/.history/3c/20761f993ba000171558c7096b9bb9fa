
public class TimePeriod {

	private int hours;
	private int minutes;

	public TimePeriod() {

	}

	public void addHours(int hoursToAdd) {

		if (hours > 0) {
			
			for (int i = 0; i < hours; i++) {
				
				hours += 1;
		
		} 
		}
	}

	public void addMinutes(int minutesToAdd) {

		
	}
	
	public String toString() {
		
		return hours + " h " + minutes + " min";
	}
}
